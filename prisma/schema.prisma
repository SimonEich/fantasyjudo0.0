// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider     = "postgresql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

generator client {
  provider = "prisma-client-js"
}


model Name {
  id       String @id @default(cuid())
  authorId String
  name     String @unique
}


model Team {
  id       String @id @default(cuid())
  authorId String
  user     String
  player   String
  weight   Int
  country  String
}

model Winner {
  id       String   @id @default(cuid())
  now      DateTime @default(now())
  authorId String
  player   String
  weight   Int      @unique
  rank     Int
  country  String
}
